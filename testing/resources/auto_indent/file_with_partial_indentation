# bad
def foo(a,
    b,
    c,
    *args,
    **kwargs
):
    pass

# okay
def bar():
    pass

# bad
_ = foo(
    1,
    2,
    3,
    4,
    e=5,
    *[],
    **{}
)

# okay
_ = bar()

# okay
_ = foo(bar, baz, womp)

# bad
class baz(
    object,
    object,
):
    pass

# okay
class qux:
    pass

# bad
_ = foo(
    foo(
        1 + foo(
            1,  # trickier
            2,
            3,
        ),
        4,
        5,
    ),
    6,
    7,
)

# bad
_ = '(wow)'.foo(
    no,
    way,
)
